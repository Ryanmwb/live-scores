<script>
    var sn = $(".side-nav");
    var pos = sn.position();
    $(window).scroll(function() {
        var windowPos = $(window).scrollTop();
        if (windowPos >= pos.top - 125) {
            sn.addClass("stick");
        } else {
            sn.removeClass("stick");
        }
    });
</script>