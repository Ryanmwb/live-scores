<!DOCTYPE html>
<html lang="en">

  <% include ../static/partials/head.ejs %>
  <body>
      <!-- Page Content -->
      <main class="container">

        <!-- Page Features -->
        <section class="scores">
              <div class="row">
                <h2 class="col-sm-5 text-right"></h2>
                <h2 class="col-sm-2 text-center font-weight-bold">Score</h2>
                <h2 class="col-sm-5 text-left"></h2>
              </div>
              <% games.forEach((game) => { %>
                <div class="row">
                  <h3 class="col-sm-5 text-right "><%= game.team1 %></h3>
                  <h5 class="col-sm-2 text-center"><%= game.score %></h5>
                  <h3 class="col-sm-5 text-left"><%= game.team2 %></h3>
                </div>
              <% }) %>
        </section>
      </main>
      <!-- /.container -->
      <% include ../static/partials/baseScripts.ejs %>
    </body>
</html>